{
  "log": {
    "disabled": false,
    "level": "trace",
    "output": "ts-test.log",
    "timestamp": true
  },
  "ntp": {
    "enabled": true,
    "server": "ntp.aliyun.com",
    "server_port": 123,
    "interval": "30m"
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "default_mode": "rule"
    }
  },
  "dns": {
    "servers": [
      {
        "type": "udp",
        "tag": "DNS-RESOLVER",
        "server": "223.5.5.5",
        "server_port": 53,
        "detour": "Direct"
      },
      {
        "type": "https",
        "tag": "DNS-PROXY",
        "server": "cloudflare-dns.com",
        "server_port": 443,
        "domain_resolver": "DNS-RESOLVER",
        "detour": "节点选择"
      },
      {
        "type": "https",
        "tag": "DNS-DIRECT",
        "server": "dns.alidns.com",
        "server_port": 443,
        "domain_resolver": "DNS-RESOLVER",
        "detour": "Direct"
      },
      {
        "type": "tailscale",
        "tag": "dns_ts",
        "endpoint": "ts-ep",
        "accept_default_resolvers": false
      },
      {
        "type": "fakeip",
        "tag": "DNS-FAKEIP",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "DNS-DIRECT"
      },
      {
        "clash_mode": "global",
        "server": "DNS-PROXY"
      },
      {
        "domain_suffix": [
          "ntp.aliyun.com",
          "ddns-link-b5.uk",
          "dexlos.com",
          "jsdmirror.cn",
          "jsdmirror.com",
          "fastly.jsdelivr.net"
        ],
        "action": "route",
        "strategy": "ipv4_only",
        "server": "DNS-DIRECT"
      },
      {
        "domain_suffix": [
          ".ts.net"
        ],
        "ip_accept_any": true,
        "server": "dns_ts"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "action": "route",
        "server": "DNS-FAKEIP",
        "disable_cache": true
      }
    ],
    "final": "DNS-PROXY"
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 4000,
      "udp_timeout": "5m"
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "singbox-tun0",
      "address": [
        "172.18.0.1/30"
      ],
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "mtu": 1400,
      "endpoint_independent_nat": true,
      "udp_timeout": "5m",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 4000
        }
      }
    }
  ],
  "outbounds": [
    {
      "tag": "节点选择",
      "type": "selector",
      "outbounds": [
        "my-claw-jp-vlwn"
      ]
    },
    {
      "tag": "my-claw-jp-vlwn",
      "type": "vless",
      "server": "pycjp.crushry.fun",
      "server_port": 443,
      "uuid": "f1c7003c-d3b7-4919-ac9f-4c374e47ad68",
      "tls": {
        "enabled": true,
        "server_name": "pycjp.crushry.fun",
        "insecure": false
      },
      "transport": {
        "type": "ws",
        "headers": {},
        "path": "/vlwn"
      }
    },
    {
      "tag": "Direct",
      "type": "direct",
      "domain_resolver": "DNS-DIRECT"
    },
    {
      "tag": "Reject",
      "type": "block"
    }
  ],
  "route": {
    "default_domain_resolver": {
      "server": "DNS-DIRECT"
    },
    "rule_set": [
      {
        "tag": "ry-rule-direct",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/rammusry/rules/raw/refs/heads/main/rules/ry-rule-direct.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "ry-rule-proxy",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/rammusry/rules/raw/refs/heads/main/rules/ry-rule-proxy.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "ry-rule-pure",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/rammusry/rules/raw/refs/heads/main/rules/ry-rule-pure.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "ry-rule-reject",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/rammusry/rules/raw/refs/heads/main/rules/ry-rule-reject.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "China",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/China/China.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "ChinaIP",
