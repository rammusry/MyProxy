      },
      {
        "tag": "Proxy",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/Proxy/Proxy.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "GoogleFCM",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/GoogleFCM/GoogleFCM.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "Google",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/Google/Google.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "Microsoft",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/Microsoft/Microsoft.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "Apple",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/Apple/Apple.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "Anthropic",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/Anthropic/Anthropic.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "OpenAI",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/OpenAI/OpenAI.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "Gemini",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/Gemini/Gemini.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "Advertising",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/senshinya/singbox_ruleset/main/rule/AdvertisingLite/AdvertisingLite.srs",
        "download_detour": "节点选择"
      }
    ],
    "rules": [
      {
        "port": 53,
        "action": "hijack-dns"
      },
      {
        "inbound": [
          "mixed-in",
          "tun-in"
        ],
        "action": "sniff",
        "timeout": "300ms"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "节点选择"
      },
      {
        "ip_cidr": [
          "100.64.0.0/10"
        ],
        "action": "route",
        "outbound": "ts-ep"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "Direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "network": [
              "udp"
            ]
          },
          {
            "port": [
              443
            ]
          }
        ],
        "action": "reject",
        "method": "default",
        "no_drop": false
      },
      {
        "domain": [
          "ntp.aliyun.com"
        ],
        "action": "route",
        "outbound": "Direct"
      },
      {
        "domain_keyword": [
          "googleapis"
        ],
        "action": "route",
        "outbound": "Google"
      },
      {
        "rule_set": [
          "ry-rule-reject",
          "Advertising"
        ],
        "action": "reject",
        "method": "default",
        "no_drop": false
      },
      {
        "rule_set": [
          "Proxy",
          "ry-rule-proxy"
        ],
        "action": "route",
        "outbound": "节点选择"
      },
      {
        "rule_set": [
          "China",
          "ChinaIP",
          "ry-rule-direct"
        ],
        "action": "route",
        "outbound": "Direct"
      }
    ],
    "final": "Direct",
    "auto_detect_interface": true,
    "override_android_vpn": true
  },
  "endpoints": [
    {
      "type": "tailscale",
      "tag": "ts-ep",
      "auth_key": "",
      "hostname": "singbox-k70u",
      "udp_timeout": "5m"
    }
  ]
